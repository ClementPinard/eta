{
    "info": {
        "name": "sample_videos",
        "type": "eta.core.types.Module",
        "version": "0.1.0",
        "description": "Module that samples video frames",
        "exe": "sample_videos.py"
    },
    "inputs": [
        {
            "name": "input_path",
            "type": "eta.core.types.Video",
            "description": "The input video",
            "required": true
        }
    ],
    "outputs": [
        {
            "name": "output_video_clips_path",
            "type": "eta.core.types.VideoClips",
            "description": "The output video clips",
            "required": false
        },
        {
            "name": "output_frames_dir",
            "type": "eta.core.types.ImageSequenceDirectory",
            "description": "A directory in which to write the sampled frames",
            "required": false
        },
        {
            "name": "output_frames_path",
            "type": "eta.core.types.ImageSequence",
            "description": "The output sampled frames",
            "required": false
        },
        {
            "name": "output_video_path",
            "type": "eta.core.types.VideoFile",
            "description": "The output sampled video",
            "required": false
        }
    ],
    "parameters": [
        {
            "name": "fps",
            "type": "eta.core.types.Number",
            "description": "The output frame rate",
            "required": false,
            "default": null
        },
        {
            "name": "clips_path",
            "type": "eta.core.types.EventDetection",
            "description": "Per-frame binary labels indicating which frames to sample",
            "required": false,
            "default": null
        }
    ]
}